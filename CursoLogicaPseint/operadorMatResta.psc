Algoritmo operadorMatResta
	//Definir Variables
	Definir a Como Entero;
	Definir b Como Entero;
	Definir Resta Como Entero; 
	
	//Darle valor a A
	Escribir "Ingrese el valor de a";
	Leer a; 
	
	//Darle valor a B
	Escribir "Ingrese el valor de b";
	Leer b;
	
	//Restar con el operador aritmético suma(-)
	Resta <- a - b;
	
	//Imprimir el resultado
	Escribir  "La resta de (a - b) es: ", Resta;
	//<-
FinAlgoritmo
