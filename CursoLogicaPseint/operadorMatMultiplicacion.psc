Algoritmo operadorMatMultiplicacion
	//Definir Variables
	Definir a Como Entero;
	Definir b Como Entero;
	Definir Multiplicacion Como Entero; 
	
	//Darle valor a A
	Escribir "Ingrese el valor de a";
	Leer a; 
	
	//Darle valor a B
	Escribir "Ingrese el valor de b";
	Leer b;
	
	//Restar con el operador aritmético suma(-)
	Multiplicacion <- a * b;
	
	//Imprimir el resultado
	Escribir  "La Multiplicacion de (a * b) es: ", Multiplicacion;
	//<->
FinAlgoritmo
