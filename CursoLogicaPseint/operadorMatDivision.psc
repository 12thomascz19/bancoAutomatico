Algoritmo operadorMatDivision
	//Definir Variables
	Definir a Como Entero;
	Definir b Como Entero;
	Definir Divivsion Como Entero; 
	
	//Darle valor a A
	Escribir "Ingrese el valor de a";
	Leer a; 
	
	//Darle valor a B
	Escribir "Ingrese el valor de b";
	Leer b;
	
	//Restar con el operador aritmético suma(-)
	Divivsion <- a / b;
	
	//Imprimir el resultado
	Escribir  "La División de (a / b) es: ", Divivsion;
	//<->
FinAlgoritmo
